pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                checkout scm: [
                    $class: 'GitSCM',
                    branches: [[name: '*/main']], // Use "main" instead of "master"
                    extensions: [],
                    userRemoteConfigs: [[url: 'https://github.com/shrutibhedasgaonkar/python_programs']]
                ]
            }
        }
    }
}
